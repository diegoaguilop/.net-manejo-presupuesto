@model TipoCuenta

@{
    ViewData["Title"] = "Eliminar Tipo Cuenta";
}

<h3 class="mb-3">¿Está seguro que desea eliminar el tipo de cuenta @Model.Nombre?</h3>

<form asp-action="EliminarTipoCuenta">
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger">Eliminar</button>
    <a class="btn btn-secondary" asp-action="Index">Cancelar</a>
</form>